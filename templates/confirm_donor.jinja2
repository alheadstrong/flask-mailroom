{% extends 'base.jinja2' %}

{% block subtitle %}Add a new donation{% endblock subtitle %}

{% block content %}
<form method="POST">
	<p> {{ donor_name }} not found. Would you like to add {{ donor_name }} to the donor list with an initial donation of {{ value }}?</p>
	<input type="submit" value="yes" name="yes"><input type="submit" value="no" name="no">
</form>
{% endblock content %}